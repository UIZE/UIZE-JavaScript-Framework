<%@ input ({idPrefix:'string',title:'string',contents:'string'}) %>
<%@ required (['UizeSite.Templates.Dialog','Uize.Templates.Calendar']) %>
<%=
	UizeSite.Templates.Dialog.process ({
		idPrefix:input.idPrefix,
		title:input.title,
		contents:input.contents,
		topLeftButtons:'<a id="' + input.idPrefix + '_keepOpen" class="dialogKeepOpenButton" title="Toggle Keep Open"></a>',
		dialogClass:'dialogPalette'
	})
%>
