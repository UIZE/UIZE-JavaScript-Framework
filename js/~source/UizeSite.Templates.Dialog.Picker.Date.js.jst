<%@ input ({idPrefix:'string',title:'string'}) %>
<%@ required (['UizeSite.Templates.Dialog.Picker','Uize.Templates.Calendar']) %>
<%=
	UizeSite.Templates.Dialog.Picker.process ({
		idPrefix:input.idPrefix,
		title:input.title,
		contents:Uize.Templates.Calendar.process ({idPrefix:input.idPrefix + '_value'})
	})
%>
